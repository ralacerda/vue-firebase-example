{
  "name": "vue3-firebase-example",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "dev:emulators": "run-p -l emulators functions",
    "emulators": "firebase emulators:start --project=\"demo-playground\"",
    "functions": "pnpm run -C functions build:watch",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "seed": "node helpers/seedDatabase.js"
  },
  "dependencies": {
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "unplugin-icons": "^0.18.1",
    "vue": "^3.3.11",
    "vuefire": "^3.1.22"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@iconify-json/mdi": "^1.1.63",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-vue-devtools": "1.0.0-rc.8",
    "vue-tsc": "^1.8.25"
  }
}
